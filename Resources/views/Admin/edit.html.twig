{% extends 'NuxiaValuelistBundle:Admin:base.html.twig' %}

{% trans_default_domain 'NuxiaValuelist' %}

{% block title_content %}
    {{ ('valuelist.' ~ category ~ '.edit.title')|trans }}
{% endblock %}

{% block seo %}
    <title>{{ ('valuelist.' ~ category ~ '.edit.title')|trans }}</title>
    <meta content="{{ ('valuelist.' ~ category ~ '.edit.title')|trans }}" name="title">
{% endblock %}

{% block content %}
    {{ include('NuxiaValuelistBundle:Admin:form.html.twig', {form: form}, with_context = false) }}
{% endblock %}
